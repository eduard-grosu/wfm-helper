{
  "manifest_version": 3,
  "name": "WFM Helper",
  "version": "1.0",
  "description": "warframe.market extension helper",
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://warframe.market/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "data/components.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}